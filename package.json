{
  "name": "doob-ng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:core": "ng build @doob-ng/core --configuration production",
    "publish:core": "npm publish ./dist/doob-ng/core --access public",
    "build:grid": "ng build @doob-ng/grid --configuration production",
    "publish:grid": "npm publish ./dist/doob-ng/grid --access public",
    "build:editor": "ng build @doob-ng/editor --configuration production",
    "publish:editor": "npm publish ./dist/doob-ng/editor --access public",
    "build:antd": "ng build @doob-ng/antd-extensions --configuration production",
    "publish:antd": "npm publish ./dist/doob-ng/antd-extensions --access public",
    "build:cdk-helper": "ng build @doob-ng/cdk-helper --configuration production",
    "publish:cdk-helper": "npm publish ./dist/doob-ng/cdk-helper --access public"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/angular": "26.2",
    "@ag-grid-community/client-side-row-model": "26.2",
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@ant-design/icons-angular": "^13.0.2",
    "@datorama/akita": "^7.1.1",
    "@datorama/akita-ng-router-store": "^7.0.0",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@fortawesome/angular-fontawesome": "^0.10.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "monaco-editor": "^0.26.1",
    "ng-zorro-antd": "^13",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular/cli": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.2.1",
    "typescript": "~4.5.5"
  }
