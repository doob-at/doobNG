{
  "name": "doob-ng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:core": "ng build @doob-ng/core --configuration production",
    "publish:core": "npm publish ./dist/doob-ng/core --access public",
    "build:grid": "ng build @doob-ng/grid --configuration production",
    "publish:grid": "npm publish ./dist/doob-ng/grid --access public",
    "build:editor": "ng build @doob-ng/editor --configuration production",
    "publish:editor": "npm publish ./dist/doob-ng/editor --access public",
    "build:antd": "ng build @doob-ng/antd-extensions --configuration production",
    "publish:antd": "npm publish ./dist/doob-ng/antd-extensions --access public",
    "build:cdk-helper": "ng build @doob-ng/cdk-helper --configuration production",
    "publish:cdk-helper": "npm publish ./dist/doob-ng/cdk-helper --access public"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/all-modules": "^26.0.0",
    "@ag-grid-community/angular": "^26.0.0",
    "@ag-grid-community/client-side-row-model": "^26.0.0",
    "@ag-grid-community/core": "^26.0.0",
    "@angular/animations": "~12.1.0-",
    "@angular/common": "~12.1.0-",
    "@angular/compiler": "~12.1.0-",
    "@angular/core": "~12.1.0-",
    "@angular/forms": "~12.1.0-",
    "@angular/platform-browser": "~12.1.0-",
    "@angular/platform-browser-dynamic": "~12.1.0-",
    "@angular/router": "~12.1.0-",
    "@datorama/akita": "^6.2.1",
    "@datorama/akita-ng-router-store": "^6.0.1",
    "@datorama/akita-ngdevtools": "^6.0.1",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "monaco-editor": "^0.26.1",
    "ng-zorro-antd": "^12.0.1",
    "rxjs": "~6.6.0",
